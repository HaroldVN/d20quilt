#see http://editorconfig.org/ for more info
#top-most editorconfig file
root=true

#default styling?
#unix-style newline
#newline ends all files
#indent with spaces, not tab
#indent size equal to four spaces
#utf-8 default charset
[*]
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 4
charset = utf-8
